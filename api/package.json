{
	"name": "server",
	"version": "1.0.0",
	"description": "Consolidated Tool Renting App",
	"main": "index.js",
	"scripts": {
		"precommit": "pretty-quick --staged",
		"start": "node index.js",
		"server": "nodemon index.js",
		"test": "jest -f 'index.test.js'",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"yarn server\" \"yarn client\"",
		"heroku-postbuild": "npm install --only=dev --prefix client && npm install --prefix client && npm run build --prefix client"
	},
	"author": "conorburke",
	"license": "MIT",
	"engines": {
		"node": "10.8.0",
		"npm": "6.1.0",
		"yarn": "1.9.4"
	},
	"dependencies": {
		"bcrypt-nodejs": "^0.0.3",
		"body-parser": "^1.18.2",
		"concurrently": "^3.6.0",
		"cookie-session": "1.3.1",
		"cors": "^2.8.5",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"express-graphql": "^0.6.12",
		"graphql": "^0.13.2",
		"knex": "^0.15.0",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0",
		"pg": "^7.4.3",
		"stripe": "^6.20.0"
	},
	"devDependencies": {
		"husky": "^0.14.3",
		"jest": "^23.2.0",
		"nodemon": "^1.17.5",
		"prettier": "1.13.7",
		"pretty-quick": "^1.6.0"
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true
	}
}
